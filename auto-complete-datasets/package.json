{
  "name": "jcl-dataset-autocomplete",
  "displayName": "JCL Dataset Autocomplete",
  "description": "Autocompletado de datasets desde archivo externo",
  "version": "1.0.0",
  "publisher": "ccasti10",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccasti10/auto-complete-datasets.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:jcl"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jclDatasetAutocomplete.selectDatasetFile",
        "title": "JCL: Seleccionar archivo de datasets"
      }
    ],
    "configuration": {
      "title": "JCL Dataset Autocomplete",
      "properties": {
        "jclDatasetAutocomplete.datasetFile": {
          "type": "string",
          "default": "D:\\jcl-datasets.txt",
          "description": "Ruta al archivo con la lista de datasets"
        }
      }
    }
  }
}